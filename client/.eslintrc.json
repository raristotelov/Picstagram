{
	"extends": ["react-app"],
	"rules": {
		"semi": ["error", "always"],
		"quotes": ["error", "single"],
		"jsx-quotes": ["error", "prefer-single"],
		"indent": ["error", "tab"],
		"comma-dangle": ["error", "always-multiline"],
		"max-len": ["error", { "code": 150 }],
		"arrow-parens": ["error", "always"],
		"react/jsx-newline": ["error", { "prevent": false }],
		"no-multiple-empty-lines": ["error", { "max": 1 }]
	}
}
